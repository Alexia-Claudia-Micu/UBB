@{
}
@model IEnumerable<Template_Log.Models.Entities.Bird>

<h2>My Birds</h2>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Species</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var bird in Model)
        {
            <tr>
                <td>@bird.Name</td>
                <td>@bird.Species</td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create">Add New Bird</a>
